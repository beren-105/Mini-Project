{
  "compilerOptions": {
    "target": "es6", 
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    
    "module": "ES2015",   
    "noImplicitAny": true,
    "strictNullChecks": true,

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "skipLibCheck": true
  }
}
